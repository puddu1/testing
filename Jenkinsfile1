pipeline {
    agent any
    stages{
        stage('hello'){
          steps{
          when {
              changeset 'WEBPC'
          }

          script{
            sh ''' echo "hello" 
            git branch
            '''
                   def DEPLOYMENT_SHARE_NAME = 'NEW WEB'
                        env.DEPLOY_SHARE_NAME = "${DEPLOYMENT_SHARE_NAME}"
                        echo "DEPLOY_SHARE_NAME: $DEPLOY_SHARE_NAME"
           }
          }

        }
    
    }
}
