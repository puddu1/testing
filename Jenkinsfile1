pipeline {
    agent any
    stages{
        stage('hello'){
          steps{
          script{
            sh ''' echo "hello" 
            git branch
            '''
                   def DEPLOYMENT_SHARE_NAME = git for-each-ref --sort=-committerdate refs/heads/ --format='%(committerdate:short)' 
                        env.DEPLOY_SHARE_NAME = "${DEPLOYMENT_SHARE_NAME}"
                        echo "DEPLOY_SHARE_NAME: $DEPLOY_SHARE_NAME"
           }
          }

        }
    
    }
}
